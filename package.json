{
  "name": "advanced-ele-ui",
  "version": "0.0.5-beta-3",
  "description": "基于 Vue3 + Element Plus 的高级组件库",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/style.css": "./dist/style.css",
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "vite",
    "check": "vue-tsc --noEmit",
    "build": "vue-tsc --noEmit && vite build",
    "build:demo": "vite build --config vite.config.demo.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "preview": "vite preview",
    "preview:demo": "vite preview --config vite.config.demo.ts --outDir dist-demo",
    "prepublishOnly": "npm run build",
    "publish:npm": "npm publish --registry=https://registry.npmjs.org/ --access public",
    "login:npm": "npm login --registry=https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vvhr/advanced-ele-ui.git"
  },
  "bugs": {
    "url": "https://github.com/vvhr/advanced-ele-ui/issues"
  },
  "homepage": "https://github.com/vvhr/advanced-ele-ui#readme",
  "keywords": [
    "vue",
    "vue3",
    "element-plus",
    "component-library",
    "typescript",
    "form",
    "table",
    "ui-components",
    "frontend"
  ],
  "author": {
    "name": "vvhr",
    "email": "vvhr_anen@163.com",
    "url": "https://github.com/vvhr"
  },
  "license": "MIT",
  "peerDependencies": {
    "element-plus": "^2.11.7",
    "vue": "^3.4.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@iconify/vue": "^5.0.0",
    "@vueuse/core": "^11.0.0",
    "aieditor": "^1.4.2",
    "dayjs": "^1.11.13",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@unocss/preset-attributify": "^66.5.4",
    "@unocss/preset-icons": "^66.5.4",
    "@unocss/preset-wind": "^66.5.4",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "element-plus": "^2.11.7",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-vue": "^9.19.2",
    "less": "^4.2.0",
    "pinia": "^2.1.7",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "unocss": "^66.5.4",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.0.0",
    "vue": "^3.4.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.1",
  "lint-staged": {
    "src/**/*.{vue,js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,less,scss,md}": [
      "prettier --write"
    ]
  }
}
