{
  "name": "advanced-ele-ui",
  "version": "0.1.9",
  "buildVersion": "dev-20260105-1629",
  "description": "Advanced Component Library Based on Vue3 + Element Plus",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./global": {
      "types": "./global.d.ts"
    },
    "./dist/style.css": "./dist/style.css",
    "./style.css": "./dist/style.css",
    "./dist/element-plus-beauty.css": "./dist/element-plus-beauty.css",
    "./element-plus-beauty.css": "./dist/element-plus-beauty.css"
  },
  "files": [
    "dist",
    "global.d.ts",
    "README.md",
    "README.zh.md",
    "LICENSE",
    "CHANGELOG.md",
    "CHANGELOG.zh.md"
  ],
  "scripts": {
    "dev": "vite",
    "dev:lib": "vite build --watch",
    "update-build": "node scripts/update-build-version.js",
    "check": "vue-tsc --noEmit",
    "build": "node scripts/update-build-version.js && vue-tsc --noEmit && vite build",
    "build:demo": "vite build --config vite.config.demo.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "preview": "vite preview",
    "preview:demo": "vite preview --config vite.config.demo.ts --outDir dist-demo",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "npm run build",
    "publish:npm": "npm publish --registry=https://registry.npmjs.org/ --access public",
    "publish:release": "npm config set registry https://registry.npmjs.org & npm run publish:npm & npm config set registry https://registry.npmmirror.com",
    "publish:beta": "npm config set registry https://registry.npmjs.org & npm publish --tag beta & npm config set registry https://registry.npmmirror.com",
    "login:npm": "npm login --registry=https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vvhr/advanced-ele-ui.git"
  },
  "bugs": {
    "url": "https://github.com/vvhr/advanced-ele-ui/issues"
  },
  "homepage": "https://github.com/vvhr/advanced-ele-ui#readme",
  "keywords": [
    "vue",
    "vue3",
    "element-plus",
    "component-library",
    "typescript",
    "form",
    "table",
    "ui-components",
    "frontend"
  ],
  "author": {
    "name": "vvhr",
    "email": "vvhr_anen@163.com",
    "url": "https://github.com/vvhr"
  },
  "license": "MIT",
  "peerDependencies": {
    "element-plus": ">=2.9.0",
    "vue": ">=3.2.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@iconify/vue": "^5.0.0",
    "@unocss/preset-wind3": "^66.5.7",
    "@vueuse/core": "^11.0.0",
    "aieditor": "^1.4.2",
    "dayjs": "^1.11.13",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@unocss/preset-attributify": "^66.5.7",
    "@unocss/preset-icons": "^66.5.7",
    "@unocss/preset-wind": "^66.5.7",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vitest/ui": "^4.0.9",
    "@vue/test-utils": "^2.4.6",
    "element-plus": "^2.11.7",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^27.2.0",
    "less": "^4.2.0",
    "pinia": "^2.1.7",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "unocss": "^66.5.7",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.0.0",
    "vitest": "^4.0.9",
    "vue": "^3.4.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.1",
  "lint-staged": {
    "src/**/*.{vue,js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,less,scss,md}": [
      "prettier --write"
    ]
  }
}
